{
  "name": "dobby-api-v2",
  "version": "0.1.0",
  "bin": {
    "dobby-api-v2": "bin/dobby-api-v2.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "ts-node deployment/deploy.ts",
    "deploy:develop": "npm run deploy -- --env develop",
    "deploy:production": "npm run deploy -- --env production",
    "deploy:backend": "npm run deploy -- --skip-frontend",
    "deploy:frontend": "npm run deploy -- --skip-backend",
    "destroy": "cdk destroy --all",
    "destroy:develop": "cdk destroy --all --profile dobby_develop -c environment=develop",
    "destroy:production": "cdk destroy --all --profile dobby_production -c environment=production",
    "bootstrap": "chmod +x scripts/bootstrap.sh && ./scripts/bootstrap.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/node": "22.7.9",
    "@types/node-fetch": "^2.6.11",
    "aws-cdk": "2.174.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.524.0",
    "@aws-sdk/client-dynamodb": "^3.724.0",
    "@aws-sdk/client-iot-wireless": "^3.724.0",
    "@aws-sdk/lib-dynamodb": "^3.724.0",
    "@aws-sdk/util-dynamodb": "^3.724.0",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.18.3",
    "@hono/zod-validator": "^0.4.2",
    "@scalar/hono-api-reference": "^0.5.165",
    "aws-cdk-lib": "2.174.1",
    "constructs": "^10.0.0",
    "hono": "^4.6.16",
    "hono-openapi": "^0.3.1",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "node-fetch": "^2.7.0",
    "zod": "^3.24.1",
    "zod-openapi": "^4.2.2"
  }
}